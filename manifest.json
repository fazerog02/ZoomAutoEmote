{
    "name": "Zoom Auto Emote",
    "description": "ユーザーの表情に基づいてZoom上で自動でエモートをする",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "https://zoom.us/wc/*/start",
            "https://zoom.us/wc/join/*"
        ],
        "js": [
            "js/tf.min.js",
            "js/teachablemachine-image.min.js",
            "js/background.js"
        ]
    }],
	"icons": {
		"16": "icons/16.png",
		"128": "icons/128.png"
	},
    "permissions": []
}